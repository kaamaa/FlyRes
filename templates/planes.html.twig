<!-- Flugzeugnamen und Flugzeugkennung ausgeben -->
    {% for plane in planes %}
     <tr>
       <td bgcolor="{{ colors['wochentag'] }}" align="left" nowrap height="17">
         <b> {{ plane.type }} </b>
       </td>
       <td bgcolor="{{ colors['wochentag'] }}" align="left" nowrap height="17">
         <b>{{ plane.kennung }} </b>
       </td>
       <!-- FÃ¼r jeden Tag die Buchungen ausgeben -->
       {% for booking in bookings %}
         {% if booking.plane == plane.planeID %}
           <td 
               {% if booking.tooltip != "" %} onmouseover="Tip('{{ booking.tooltip }}')" onmouseout="UnTip()" {% endif %} 
               bgcolor="{{ colors[booking.color] }}" align="center" height="17" 
               onClick="javascript:document.form{{ booking.plane }}{{ booking.day }}.submit();"
           >
             <form novalidate="true" action="dayview" name="form{{ booking.plane }}{{ booking.day }}" method="POST">
               <input type="hidden" name="ts" value="{{ booking.plane }}_{{ booking.bookingdate }}">
               <a href="javascript:document.form{{ booking.plane }}{{ booking.day }}.submit();">
                 <img src="{{ asset('images/nichts.gif') }}" width="100%" height="100%" border="0">
               </a>
             </form>  
           </td>
         {% endif %}
       {% endfor %} 
     </tr>
    {% endfor %} 